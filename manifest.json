{
  "manifest_version": 2,
  "name": "twitterdiffs",
  "version": "1.0",

  "description": "edited tweet history diff highlighting ➕➖",

  "icons": {
    "48": "icons/twitterdiffs-48.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "twitterdiffs@mmyron.com"
    }
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*/status/*/history"
      ],
      "js": ["script.js"]
    }
  ]
}
